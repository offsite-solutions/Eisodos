{
  "config": {
    "TemplateDir": "$_applicationDir/templates/",
    "AllowAdmin": "127.0.0.1",
    "RewriteEnabled": false,
    "EnableInlineCallback": true,
    "EnableTemplateAbs": true,
    "EnableParamCallback": true,
    "DebuggerStorage": "$_applicationDir/logs/$_applicationname-PHPConsole.storage",
    "DebugMessages": false,
    "DebugPassword": null,
    "DebugErrors": false,
    "DebugExceptions": false,
    "ErrorOutput": "File,Mail",
    "ErrorMailTo": "$_applicationName@example.com",
    "ErrorMailFrom": "$_applicationName@example.com",
    "ErrorLog": "$_applicationDir/logs/$_applicationName-error.log",
    "Langs": null,
    "LangIDFile": null,
    "UserLangIDFile": null,
    "MultiLang": false,
    "TranslateLanguageTags": true,
    "DefLang": "HU",
    "DefTemplateLang": "HU",
    "CollectParamsToFile": null,
    "DebugMissingParams": false,
    "TrimInputParams": true,
    "LoopCount": 1000,
    "AlwaysNoCache": false,
    "ShowMissingTemplate": false,
    "Cookie_Domain": null,
    "IncludeStatistic": false,
    "TitleString": "%TITLE%",
    "TitleCapitalString": "%TITLECAP%",
    "TitleReplace": "<h1>",
    "TitleCut": true,
    "TitleEmptyHU": "no predefined title",
    "TitleEmptyEN": "no predefined title",
    "TitleConcat": true,
    "DescriptionString": "%DESC%",
    "DescriptionReplace": "<desc>"
  }
}